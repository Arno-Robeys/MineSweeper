<UserControl x:Class="View.screens.Home"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:View.screens"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <DockPanel>
        <Button DockPanel.Dock="Top" Content="Start Game" Command="{Binding Settings}" />
        <Button DockPanel.Dock="Top" Content="Start Game with standard setings" Command="{Binding StartGame}" />
        <TextBlock FontSize="64" Text="Home" HorizontalAlignment="Center"  VerticalAlignment="Center"/>
    </DockPanel>
</UserControl>
